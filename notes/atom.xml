---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Fang-Lin's Notes</title>
  <link href="/notes/atom.xml" rel="self"/>
  <link href="/notes" rel="self">
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://fanglinhe.github.io/notes</id>
  <author>
    <name>Fang-Lin He</name>
  </author>

  {% for post in site.posts %}
    <entry>
      <title>{{ post.title }}</title>
      <link href="http://fanglinhe.github.io{{ post.url }}"/>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <id>http://fanglinhe.github.io{{ post.id }}</id>
      <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
  {% endfor %}

</feed>

